<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.yourcompany.questexport">

    <!-- INTERNET permission may be added by Unity/library; keep only storage permission here to avoid duplicate declarations -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!-- Use the generated app_name string resource so manifest merger doesn't collide with unityLibrary -->
    <application android:label="@string/app_name" tools:replace="android:label">
    </application>
</manifest>
