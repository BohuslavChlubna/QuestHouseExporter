<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.yourcompany.questexport">

    <!-- INTERNET permission may be added by Unity/library; keep only storage permission here to avoid duplicate declarations -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!-- Use a literal app label and request manifest merger to replace any other label values -->
    <application android:label="QuestExporter" tools:replace="android:label">
    </application>
</manifest>
